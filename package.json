{
  "name": "ssr",
  "version": "0.0.1",
  "description": "The React SSR Starter App",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git://github.com/tgrant59/react-ssr-starter.git"
  },
  "engines": {
    "npm": ">=3",
    "node": ">=8"
  },
  "author": "Tom Grant",
  "scripts": {
    "postinstall": "yarn build:dll",
    "prebuild": "yarn build:clean",
    "build": "node_modules/.bin/cross-env NODE_ENV=production webpack --config internals/webpack/webpack.prod.babel.js --color -p --hide-modules",
    "postbuild": "yarn ssr:render-static-pages",
    "build:clean": "node_modules/.bin/rimraf ./build && yarn ssr:clean",
    "build:dll": "node ./internals/scripts/dependencies.js",
    "start": "node_modules/.bin/cross-env NODE_ENV=development npm-run-all --parallel ssr:generate:watch ssr:start start:dev",
    "start:dev": "node_modules/.bin/cross-env NODE_ENV=development node server",
    "start:tunnel": "node_modules/.bin/cross-env NODE_ENV=development ENABLE_TUNNEL=true node server",
    "start:production": "node_modules/.bin/cross-env NODE_ENV=production node server",
    "ssr:clean": "node_modules/.bin/rimraf server/middlewares/generated.*",
    "ssr:generate": "node_modules/.bin/cross-env NODE_ENV=development webpack --progress --config internals/webpack/webpack.ssr.babel.js",
    "ssr:generate:watch": "yarn ssr:generate -- --watch",
    "ssr:render-static-pages": "node_modules/.bin/babel-node --presets stage-0 -- ./internals/scripts/render-static-pages.js",
    "ssr:start": "node_modules/.bin/cross-env NODE_ENV=development node server/devRenderService.js",
    "pretest": "yarn test:clean && yarn lint",
    "test": "node_modules/.bin/cross-env NODE_ENV=test jest --coverage",
    "test:clean": "node_modules/.bin/rimraf ./artifacts",
    "test:watch": "node_modules/.bin/cross-env NODE_ENV=test jest --watchAll",
    "test:precommit": "node_modules/.bin/cross-env NODE_ENV=test jest --bail --findRelatedTests",
    "test:ci": "node_modules/.bin/cross-env NODE_ENV=test jest --coverage",
    "docker:build": "yarn docker:build-image:base --squash && yarn docker:build-image:dev",
    "docker:build:production": "yarn docker:build-image:base --squash && yarn docker:build-image:prod --squash",
    "docker:build:ci": "yarn docker:build-image:base && yarn docker:build-image:ci",
    "docker:build-image:base": "docker build . -f internals/docker/base.Dockerfile -t ssr-base",
    "docker:build-image:dev": "docker volume create ssr && docker build . -f internals/docker/dev.Dockerfile -t ssr-dev",
    "docker:build-image:prod": "docker build . -f internals/docker/prod.Dockerfile -t ssr-production",
    "docker:build-image:ci": "docker build . -f internals/docker/ci.Dockerfile -t ssr-production",
    "docker:watch": "if [ ! \"$(docker ps | grep ssr-sync)\" ]; then yarn docker:watch:start; fi;",
    "docker:watch:start": "docker-sync start --dir=./internals/docker/.docker-sync -c=./internals/docker/docker-sync.yml",
    "docker:connect": "yarn docker:watch && docker-compose -f internals/docker/docker-compose.yml run ssr bash",
    "docker:start": "yarn docker:watch && docker-compose -f internals/docker/docker-compose.yml up ssr",
    "docker:destroy": "internals/scripts/helpers/confirm 'This will destroy ALL docker containers, images, and volumes. Are you sure?' && docker-clean all",
    "lint": "yarn lint:js && yarn lint:css",
    "lint:fix": "yarn lint:js -- --fix",
    "lint:eslint": "node_modules/.bin/eslint --ignore-path '.gitignore' --ignore-pattern 'internals/scripts' --ignore-pattern 'generated.*'",
    "lint:stylelint": "node_modules/.bin/stylelint",
    "lint:js": "yarn lint:eslint -- .",
    "lint:css": "yarn lint:stylelint -- \"'./app/**/*.js'\"",
    "lint:js:ci": "yarn lint:js -- --format junit --output-file artifacts/test_results/eslint/eslint.junit.xml",
    "lint:css:ci": "node_modules/.bin/stylelint './app/**/*.js' --custom-formatter node_modules/stylelint-junit-formatter > artifacts/test_results/stylelint/stylelint.junit.xml",
    "preanalyze": "yarn analyze:clean",
    "analyze": "node ./internals/scripts/analyze.js",
    "analyze:clean": "node_modules/.bin/rimraf stats.json",
    "precommit": "lint-staged",
    "clean:all": "yarn analyze:clean && yarn test:clean && yarn build:clean && yarn ssr:clean",
    "coveralls": "cat ./coverage/lcov.info | node_modules/.bin/coveralls",
    "extract-intl": "node_modules/.bin/babel-node --presets env,stage-0 -- ./internals/scripts/extract-intl.js",
    "generate": "node_modules/.bin/plop --plopfile internals/generators/index.js",
    "scripts": "node_modules/.bin/list-scripts",
    "open:coverage": "open ./coverage/lcov-report/index.html"
  },
  "lint-staged": {
    "*.js": [
      "yarn lint:eslint",
      "yarn lint:stylelint",
      "yarn test:precommit"
    ]
  },
  "dllPlugin": {
    "path": "node_modules/react-boilerplate-dlls",
    "exclude": [
      "chalk",
      "compression",
      "cross-env",
      "express",
      "ip",
      "minimist",
      "sanitize.css"
    ],
    "include": [
      "core-js",
      "lodash",
      "eventsource-polyfill"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "app/**/*.{js,jsx}",
      "!app/**/*.test.{js,jsx}",
      "!app/*/RbGenerated*/*.{js,jsx}",
      "!app/app.js",
      "!app/routes.js",
      "!app/global-styles.js",
      "!app/setup/ensureIntlSupport.js",
      "!app/setup/openSansObserver.js"
    ],
    "coverageDirectory": "./artifacts/coverage",
    "coverageThreshold": {
      "global": {
        "statements": 98,
        "branches": 91,
        "functions": 98,
        "lines": 98
      }
    },
    "moduleDirectories": [
      "node_modules",
      "app"
    ],
    "moduleNameMapper": {
      ".*\\.(css|less|styl|scss|sass)$": "<rootDir>/internals/mocks/cssModule.js",
      ".*\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/internals/mocks/image.js"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/internals/testing/test-bundler.js",
    "testRegex": "tests/.*\\.test\\.js$",
    "testResultsProcessor": "./node_modules/jest-junit"
  },
  "jest-junit": {
    "suiteName": "React SSR Starter Tests",
    "output": "./artifacts/test_results/jest/jest.junit.xml"
  },
  "dependencies": {
    "babel-polyfill": "6.22.0",
    "chalk": "1.1.3",
    "compression": "1.6.2",
    "cross-env": "3.1.4",
    "express": "4.14.1",
    "fontfaceobserver": "2.0.8",
    "history": "3.2.1",
    "htmlescape": "1.1.1",
    "immutable": "3.8.1",
    "intl": "1.2.5",
    "invariant": "2.2.2",
    "ip": "1.1.4",
    "isomorphic-fetch": "2.2.1",
    "lodash": "4.17.4",
    "minimist": "1.2.0",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "react-helmet": "4.0.0",
    "react-intl": "2.2.3",
    "react-redux": "5.0.2",
    "react-router": "3.0.2",
    "react-router-redux": "4.0.8",
    "react-router-scroll": "0.4.1",
    "redux": "3.6.0",
    "redux-immutable": "3.0.11",
    "redux-saga": "0.14.3",
    "reselect": "2.5.4",
    "sanitize.css": "4.1.0",
    "styled-components": "^2.1.0",
    "warning": "3.0.0",
    "whatwg-fetch": "2.0.2"
  },
  "devDependencies": {
    "assets-webpack-plugin": "3.5.1",
    "babel-cli": "6.22.2",
    "babel-core": "6.22.1",
    "babel-eslint": "7.1.1",
    "babel-loader": "6.2.10",
    "babel-plugin-dynamic-import-node": "1.0.0",
    "babel-plugin-react-intl": "2.3.1",
    "babel-plugin-react-transform": "2.0.2",
    "babel-plugin-styled-components": "1.1.4",
    "babel-plugin-transform-es2015-modules-commonjs": "6.22.0",
    "babel-plugin-transform-react-constant-elements": "6.22.0",
    "babel-plugin-transform-react-inline-elements": "6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "0.2.12",
    "babel-preset-env": "1.4.0",
    "babel-preset-react": "6.22.0",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-0": "6.22.0",
    "cheerio": "0.22.0",
    "child_process": "^1.0.2",
    "circular-dependency-plugin": "2.0.0",
    "coveralls": "2.11.15",
    "cross-spawn": "^5.1.0",
    "css-loader": "0.26.1",
    "enzyme": "2.7.1",
    "eslint": "3.13.1",
    "eslint-config-airbnb": "14.0.0",
    "eslint-config-airbnb-base": "11.0.1",
    "eslint-config-prettier": "^2.3.0",
    "eslint-import-resolver-webpack": "0.8.1",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "3.0.2",
    "eslint-plugin-prettier": "^2.1.2",
    "eslint-plugin-react": "6.9.0",
    "eslint-plugin-redux-saga": "0.3.0",
    "eventsource-polyfill": "0.9.6",
    "exports-loader": "0.6.3",
    "extract-text-webpack-plugin": "2.0.0",
    "file-loader": "0.10.0",
    "html-loader": "0.4.4",
    "html-webpack-plugin": "2.28.0",
    "http-proxy": "1.15.2",
    "husky": "^0.14.2",
    "image-webpack-loader": "2.0.0",
    "imports-loader": "0.7.0",
    "jest-cli": "18.1.0",
    "jest-junit": "^1.5.1",
    "lint-staged": "3.3.0",
    "list-scripts": "^0.0.4",
    "mkdirp": "^0.5.1",
    "ngrok": "2.2.6",
    "node-fetch": "^1.7.0",
    "node-plop": "0.5.5",
    "nodemon": "1.11.0",
    "npm-run-all": "4.0.2",
    "null-loader": "0.1.1",
    "offline-plugin": "4.6.1",
    "plop": "1.7.4",
    "prettier": "^1.5.2",
    "react-addons-test-utils": "15.4.2",
    "rimraf": "2.5.4",
    "server-destroy": "1.0.1",
    "shelljs": "0.7.6",
    "sinon": "2.0.0-pre.5",
    "style-loader": "0.13.1",
    "stylelint": "^7.12.0",
    "stylelint-config-standard": "^16.0.0",
    "stylelint-junit-formatter": "^0.2.0",
    "stylelint-processor-styled-components": "^0.1.2",
    "tree-kill": "^1.1.0",
    "url-loader": "0.5.7",
    "webpack": "2.2.1",
    "webpack-dev-middleware": "1.10.0",
    "webpack-hot-middleware": "2.17.1",
    "webpack-node-externals": "1.5.4"
  }
}
